<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical" >

    <TabHost android:id="@android:id/tabhost"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:background="#444444">

        <LinearLayout android:layout_width="fill_parent"
            android:id="@+id/linearLayout1"
            android:layout_height="fill_parent"
            android:orientation="vertical">

            <TabWidget android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:id="@android:id/tabs">
            </TabWidget>

            <FrameLayout android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:id="@android:id/tabcontent">


                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
                    android:paddingRight="@dimen/activity_horizontal_margin"
                    android:paddingTop="@dimen/activity_vertical_margin"
                    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".MainActivity"
                    android:clickable="false"
                    android:background="@drawable/france"
                    android:id="@+id/contentlayout1"
                    android:layout_gravity="left|bottom">

                    <ImageView
                        android:layout_width="2000dp"
                        android:layout_height="600dp"
                        android:id="@+id/imageView2"
                        android:layout_centerVertical="true"
                        android:layout_alignParentEnd="true" />

                    <TextView
                        android:layout_width="160dp"
                        android:layout_height="90dp"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:text="0&apos;00"
                        android:id="@+id/rap1"
                        android:textSize="80dp"
                        android:background="#222222"
                        android:textColor="#00ff00"
                        android:gravity="right"
                        android:layout_alignTop="@+id/target1"
                        android:layout_alignStart="@+id/rap2"
                        android:textStyle="bold" />

                    <TextView
                        android:layout_width="200dp"
                        android:layout_height="90dp"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:text="+0"
                        android:id="@+id/time1"
                        android:textSize="80dp"
                        android:textColor="#ff0000"
                        android:background="#444444"
                        android:gravity="right"
                        android:layout_below="@+id/textView8"
                        android:layout_toEndOf="@+id/rap1"
                        android:textStyle="bold" />

                    <TextView
                        android:layout_width="160dp"
                        android:layout_height="90dp"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:text="0&apos;00"
                        android:id="@+id/rap2"
                        android:textSize="80dp"
                        android:background="#444444"
                        android:textColor="#00ff00"
                        android:gravity="right"
                        android:layout_below="@+id/rap1"
                        android:layout_toEndOf="@+id/target1"
                        android:textStyle="bold" />

                    <TextView
                        android:layout_width="160dp"
                        android:layout_height="90dp"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:text="0&apos;00"
                        android:id="@+id/rap4"
                        android:background="#222222"
                        android:textColor="#00ff00"
                        android:gravity="right"
                        android:textSize="80dp"
                        android:layout_alignTop="@+id/target4"
                        android:layout_toEndOf="@+id/target4"
                        android:textStyle="bold" />

                    <TextView
                        android:layout_width="160dp"
                        android:layout_height="90dp"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:text="0&apos;00"
                        android:id="@+id/rap3"
                        android:textSize="80dp"
                        android:background="#444444"
                        android:textColor="#00ff00"
                        android:gravity="right"
                        android:textStyle="bold"
                        android:layout_alignTop="@+id/target3"
                        android:layout_alignStart="@+id/rap4" />

                    <TextView
                        android:layout_width="200dp"
                        android:layout_height="90dp"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:text="+0"
                        android:id="@+id/time2"
                        android:textSize="80dp"
                        android:textColor="#ff0000"
                        android:background="#222222"
                        android:gravity="right"
                        android:layout_alignTop="@+id/rap2"
                        android:layout_toEndOf="@+id/rap2"
                        android:textStyle="bold" />

                    <TextView
                        android:layout_width="200dp"
                        android:layout_height="90dp"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:text="+0"
                        android:id="@+id/time4"
                        android:textSize="80dp"
                        android:textColor="#ff0000"
                        android:background="#222222"
                        android:gravity="right"
                        android:textStyle="bold"
                        android:layout_alignTop="@+id/rap3"
                        android:layout_toEndOf="@+id/rap3" />

                    <TextView
                        android:layout_width="200dp"
                        android:layout_height="90dp"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:text="+0"
                        android:id="@+id/time3"
                        android:textSize="80dp"
                        android:textColor="#ff0000"
                        android:background="#444444"
                        android:gravity="right"
                        android:layout_alignBottom="@+id/rap4"
                        android:layout_toEndOf="@+id/rap4"
                        android:textStyle="bold" />

                    <TextView
                        android:layout_width="160dp"
                        android:layout_height="90dp"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:text="0&apos;00"
                        android:id="@+id/total"
                        android:textSize="80dp"
                        android:background="#222222"
                        android:textColor="#00ff00"
                        android:gravity="right"
                        android:textStyle="bold"
                        android:layout_below="@+id/textView6"
                        android:layout_toStartOf="@+id/rest" />

                    <Button
                        android:layout_width="425dp"
                        android:layout_height="120dp"
                        android:text="GO!!"
                        android:id="@+id/button"
                        android:textSize="80dp"
                        android:background="#999999"
                        android:layout_below="@+id/time4"
                        android:layout_alignEnd="@+id/imageView2" />

                    <TextView
                        android:layout_width="200dp"
                        android:layout_height="90dp"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:text="+000"
                        android:id="@+id/rest"
                        android:textSize="80dp"
                        android:textColor="#ff0000"
                        android:background="#444444"
                        android:gravity="right"
                        android:textStyle="bold"
                        android:layout_alignTop="@+id/total"
                        android:layout_alignEnd="@+id/imageView2" />

                    <TextView
                        android:layout_width="160dp"
                        android:layout_height="90dp"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:text="0&apos;00"
                        android:id="@+id/target1"
                        android:textSize="80dp"
                        android:background="#444444"
                        android:textColor="#ffff00"
                        android:gravity="right"
                        android:layout_alignTop="@+id/textView2"
                        android:layout_toEndOf="@+id/textView2"
                        android:textStyle="bold" />

                    <TextView
                        android:layout_width="160dp"
                        android:layout_height="90dp"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:text="0&apos;00"
                        android:id="@+id/target2"
                        android:textSize="80dp"
                        android:background="#222222"
                        android:textColor="#ffff00"
                        android:gravity="right"
                        android:layout_alignTop="@+id/textView3"
                        android:layout_toEndOf="@+id/textView3"
                        android:textStyle="bold" />

                    <TextView
                        android:layout_width="160dp"
                        android:layout_height="90dp"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:text="0&apos;00"
                        android:id="@+id/target"
                        android:textSize="80dp"
                        android:background="#444444"
                        android:textColor="#ffff00"
                        android:gravity="right"
                        android:textStyle="bold"
                        android:layout_below="@+id/textView6"
                        android:layout_toStartOf="@+id/total" />

                    <TextView
                        android:layout_width="160dp"
                        android:layout_height="90dp"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:text="0&apos;00"
                        android:id="@+id/target4"
                        android:textSize="80dp"
                        android:background="#444444"
                        android:textColor="#ffff00"
                        android:gravity="right"
                        android:layout_below="@+id/target2"
                        android:layout_toEndOf="@+id/textView3"
                        android:textStyle="bold" />

                    <TextView
                        android:layout_width="160dp"
                        android:layout_height="90dp"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:text="0&apos;00"
                        android:id="@+id/target3"
                        android:textSize="80dp"
                        android:background="#222222"
                        android:textColor="#ffff00"
                        android:gravity="right"
                        android:layout_below="@+id/rap4"
                        android:layout_toStartOf="@+id/rap4"
                        android:textStyle="bold" />

                    <TextView
                        android:layout_width="80dp"
                        android:layout_height="70dp"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:id="@+id/textView"
                        android:textSize="80dp"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentStart="true"
                        android:background="#444444"
                        android:textColor="#000000"
                        android:textStyle="bold"
                        android:layout_marginTop="20dp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="90dp"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:text="①"
                        android:id="@+id/textView2"
                        android:textSize="80dp"
                        android:layout_below="@+id/textView"
                        android:layout_alignParentStart="true"
                        android:background="#222222"
                        android:textColor="#999999" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="90dp"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:text="②"
                        android:id="@+id/textView3"
                        android:textSize="80dp"
                        android:layout_below="@+id/textView2"
                        android:layout_alignParentStart="true"
                        android:background="#444444"
                        android:textColor="#999999" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="90dp"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:text="③"
                        android:id="@+id/textView4"
                        android:textSize="80dp"
                        android:layout_below="@+id/textView3"
                        android:layout_alignParentStart="true"
                        android:background="#222222"
                        android:textColor="#999999" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="90dp"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:text="④"
                        android:id="@+id/textView5"
                        android:textSize="80dp"
                        android:layout_below="@+id/textView4"
                        android:layout_alignParentStart="true"
                        android:background="#444444"
                        android:textColor="#999999" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="90dp"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:text="TOTAL"
                        android:id="@+id/textView6"
                        android:textSize="80dp"
                        android:background="#222222"
                        android:textColor="#999999"
                        android:textStyle="bold"
                        android:layout_alignTop="@+id/textView9"
                        android:layout_alignStart="@+id/target" />

                    <TextView
                        android:layout_width="160dp"
                        android:layout_height="70dp"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:text="基準"
                        android:id="@+id/textView7"
                        android:textSize="60dp"
                        android:layout_above="@+id/textView2"
                        android:layout_toEndOf="@+id/textView"
                        android:gravity="center_horizontal"
                        android:background="#222222"
                        android:textColor="#999999"
                        android:textStyle="bold" />

                    <TextView
                        android:layout_width="160dp"
                        android:layout_height="70dp"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:text="経過"
                        android:id="@+id/textView8"
                        android:textSize="60dp"
                        android:layout_alignBottom="@+id/textView7"
                        android:layout_toEndOf="@+id/textView7"
                        android:gravity="center_horizontal"
                        android:background="#444444"
                        android:textColor="#999999"
                        android:textStyle="bold" />

                    <TextView
                        android:layout_width="200dp"
                        android:layout_height="70dp"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:text="残り"
                        android:id="@+id/textView9"
                        android:textSize="60dp"
                        android:gravity="center_horizontal"
                        android:background="#222222"
                        android:textColor="#999999"
                        android:textStyle="bold"
                        android:layout_alignTop="@+id/textView8"
                        android:layout_alignStart="@+id/time1" />

                    <ImageView
                        android:layout_width="500dp"
                        android:layout_height="250dp"
                        android:id="@+id/imageView"
                        android:src="@drawable/ma"
                        android:layout_above="@+id/button"
                        android:layout_alignParentEnd="true" />

                </RelativeLayout>

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:id="@+id/contentlayout2"
                    android:layout_gravity="left|center_vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:text="Longitude"
                        android:id="@+id/longitude"
                        android:textSize="80dp"
                        />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:text="Latitude"
                        android:id="@+id/latitude"
                        android:textSize="80dp"
                        />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:text="geotime"
                        android:id="@+id/geo_time"
                        android:textSize="80dp"
                        />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:text="Provider"
                        android:id="@+id/provider"
                        android:textSize="80dp"
                        />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:text="Message"
                        android:id="@+id/message"
                        android:textSize="80dp"
                        />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:text="Enabled"
                        android:id="@+id/enabled"
                        android:textSize="80dp"
                        />

                </LinearLayout>

            </FrameLayout>
        </LinearLayout>
    </TabHost>
</LinearLayout>
